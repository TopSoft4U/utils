{
  "name": "@topsoft4u/utils",
  "license": "UNLICENSED",
  "version": "1.0.1",
  "description": "Hooks and stuff",
  "repository": "git://github.com/TopSoft4U/utils.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "author": "≈Åukasz Kolasa",
  "files": [
    "dist",
    "esm"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "module": "esm/index.js",
  "scripts": {
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir esm",
    "clean": "rimraf dist esm",
    "build": "yarn run build:cjs",
    "watch": "rimraf dist tsconfig.tsbuildinfo && tsc -w --incremental",
    "preversion": "yarn run clean && yarn run lint && yarn run build",
    "postpublish": "git push --all origin -u && git push --tags origin",
    "lint": "eslint --ext js,jsx,ts,tsx src --fix"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "query-string": "^5.1.1"
  },
  "devDependencies": {
    "@types/node": "^14.6.4",
    "@types/query-string": "^5.1.0",
    "@types/react": "^16.9.49",
    "@types/react-router-dom": "^5.1.5"
  },
  "peerDependencies": {
  }
}
